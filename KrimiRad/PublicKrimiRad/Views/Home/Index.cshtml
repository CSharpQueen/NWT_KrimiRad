@{
    ViewBag.Title = "Krimi rad";
}

<div class="row">

    <ng-view>

    </ng-view>

    <br />

    <br />

    <h5 class="jumbotron text-center">Krimi rad</h5>

    <div ng-controller="MapCtrl" class="row">
        <div map-lazy-load="https://maps.google.com/maps/api/js">
            <ng-map style="height:600px; width:100%" center="43.849886, 18.355413" zoom="12">
                <marker ng-repeat="p in prijave" icon="{{ p.Rijesen ? '/Content/img/circle_green.png' : '/Content/img/circle_red.png'}}" position="[{{p.Latituda}} , {{p.Longituda}}]" title="{{p.TipDjelaNaziv}}" id="{{p.ID}}"></marker>
            </ng-map>
        </div>
    </div>

</div>

           


